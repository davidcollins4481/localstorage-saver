{
    "manifest_version": 2,
    "name": "LocalStorage Saver",
    "description": "Save all data in localstorage and allow it to be restored when needed",
    "version": "0.1",
    "icons": {
        "16":  "icon_16.png",
        "48":  "icon_48.png",
        "128": "icon_128.png"
    },

    "browser_action": {
        "default_icon": "icon_128.png",
        "default_popup": "popup.html"
    },
    "permissions": [
    ]
}
